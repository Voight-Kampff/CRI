<% @input_items= Inputset.all %>
<% if @input_items.any? %>
	<ol class="microposts">
		<div class="accordion" id="accordion2">
			<% @input_items.count.times do |i| %>
				<% @inputset = @input_items[i] %>
				<%=render partial: 'inputs/input_item_all' %>
			<% end %>
		</div>
	</ol>
	
<% else %>
	<p> Data Sets that you are responsible for updating will appear here </p>
<% end %>