<% provide(:title, @user.name) %>
<% if signed_in? %>
<% @kpi=Kpi.new %>
<% @input=Input.new %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>
  <div>
    <ul id="myTab" class="nav nav-tabs">
            <li class="active"><a href="#mykpis" data-toggle="tab">My KPIs</a></li>
            <li class=""><a href="#mydata" data-toggle="tab">My Data</a></li>
          </ul>
    <div id="myTabContent" class="tab-content">
                <div class="tab-pane fade active in" id="mykpis">
                  <%= render 'kpis/user_set_feed'%>
             </div>
                <div class="tab-pane fade" id="mydata">
                  <%= render 'inputs/user_set_feed'%>
                </div>
              </div>
  </div>
<% else %>
<p> please sign in </p>
<% end %>
