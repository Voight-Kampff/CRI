<%= provide(:title, 'KPI') %>
<% if signed_in? %>
<h1>Key Performance Indexes</h1>
<%= render 'kpis/form' %>

<ul id="myTab" class="nav nav-tabs">
            <li class="active"><a href="#mykpis" data-toggle="tab">My KPIs</a></li>
            <li class=""><a href="#tracked" data-toggle="tab">Tracked KPIs</a></li>
            <li class=""><a href="#all" data-toggle="tab">All</a></li>
            <li class=""><a href="#search" data-toggle="tab">Search</a></li>
          </ul>
		<div id="myTabContent" class="tab-content">
		            <div class="tab-pane fade active in" id="mykpis">
		            	<%= render 'kpis/my_set_feed'%>
			       </div>
		            <div class="tab-pane fade" id="all">
		            	<%= render 'kpis/all_feed' %>
		            </div>
		            <div class="tab-pane fade" id="tracked">
		              	<%= render 'kpis/tracked_feed' %>
		            </div>
		            <div class="tab-pane fade" id="search">
		            	<%= render 'kpis/search_feed.html.erb' %>
		            </div>
		          </div>

<% else %>
<p> fail </p>
<% end %>